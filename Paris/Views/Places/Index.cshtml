@using Log.Models

<h1> Visit Log </h1>
<hr>

@if (Model.Count == 0)
{
    <h1>go away</h1>
}
else
{
    @foreach (Place place in Model)
    {
        
        <p>@place.CityCountry</p>
        <p>@place.Year</p>
        <p>@place.Landmark</p>
        <p>@place.Notes</p>
        
    }
}

<a href= '/'>Go back</a>